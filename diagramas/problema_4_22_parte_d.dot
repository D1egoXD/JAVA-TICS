digraph Inventario_Parte_D {
    rankdir=TB;
    node [shape=box, style=filled, fontname="Arial", fontsize=10];
    edge [fontname="Arial", fontsize=9];

    // Nodos
    conectorEliminar [label="ELIMINAR", shape=cds, fillcolor="#ADD8E6", color=black, fontcolor=black];
    initI [label="I ← 1", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    converge1 [label="", shape=circle, width=0.2, height=0.2, fillcolor=black, color=black];
    cicloI [label="I ≤ N", shape=diamond, fillcolor="#ADD8E6", color=black, fontcolor=black];
    condicionCero [label="CAN[I] = 0", shape=diamond, fillcolor="#ADD8E6", color=black, fontcolor=black];
    initJ [label="J ← I", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    converge2 [label="", shape=circle, width=0.2, height=0.2, fillcolor=black, color=black];
    cicloJ [label="J ≤ N", shape=diamond, fillcolor="#ADD8E6", color=black, fontcolor=black];
    desplazar [label="PRO[J] ← PRO[J + 1]\nCAN[J] ← CAN[J + 1]", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    incrementoJ [label="J ← J + 1", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    decrementarN [label="N ← N - 1", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    incrementoI [label="I ← I + 1", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    fin [label="FIN", shape=oval, fillcolor="#F08080", color=black, fontcolor=black];

    // Conexiones
    conectorEliminar -> initI;
    initI -> converge1;
    converge1 -> cicloI;
    cicloI -> condicionCero [label="Si"];
    cicloI -> fin [label="No"];
    condicionCero -> initJ [label="Si"];
    condicionCero -> incrementoI [label="No"];
    initJ -> converge2;
    converge2 -> cicloJ;
    cicloJ -> desplazar [label="Si"];
    cicloJ -> decrementarN [label="No"];
    desplazar -> incrementoJ;
    incrementoJ -> converge2;
    decrementarN -> converge1;
    incrementoI -> converge1;
}
