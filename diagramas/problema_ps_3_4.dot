digraph TablaMultiplicar {
    rankdir=TB;
    node [shape=box, style=filled, fontname="Arial", fontsize=10];
    edge [fontname="Arial", fontsize=9];
    
    // Nodos
    inicio [label="INICIO", shape=oval, fillcolor="#90EE90", color=black, fontcolor=black];
    leerK [label="Leer K", shape=parallelogram, fillcolor="#FFFACD", color=black, fontcolor=black];
    init [label="I ← 1", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    decision [label="I ≤ 10", shape=diamond, fillcolor="#ADD8E6", color=black, fontcolor=black];
    calcular [label="RESULTADO ← K × I", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    escribir [label="Escribir\nK, I, RESULTADO", shape=parallelogram, fillcolor="#FFFACD", color=black, fontcolor=black];
    incrementar [label="I ← I + 1", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    fin [label="FIN", shape=oval, fillcolor="#F08080", color=black, fontcolor=black];
    
    // Conexiones
    inicio -> leerK;
    leerK -> init;
    init -> decision;
    decision -> calcular [label="Sí"];
    decision -> fin [label="No"];
    calcular -> escribir;
    escribir -> incrementar;
    incrementar -> decision;
}
