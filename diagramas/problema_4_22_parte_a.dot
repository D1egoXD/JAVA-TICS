digraph Inventario_Parte_A {
    rankdir=TB;
    node [shape=box, style=filled, fontname="Arial", fontsize=10];
    edge [fontname="Arial", fontsize=9];

    // Nodos
    inicio [label="INICIO", shape=oval, fillcolor="#90EE90", color=black, fontcolor=black];
    msgN [label="Escribir\n\"Ingrese el número\nde productos\"", shape=parallelogram, fillcolor="#FFFACD", color=black, fontcolor=black];
    leerN [label="Leer N", shape=parallelogram, fillcolor="#FFFACD", color=black, fontcolor=black];
    validacion [label="N ≥ 1 y\nN ≤ 100\n?", shape=diamond, fillcolor="#ADD8E6", color=black, fontcolor=black];
    initI [label="I ← 0", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    converge1 [label="", shape=circle, width=0.2, height=0.2, fillcolor=black, color=black];
    ciclo [label="I < N\n?", shape=diamond, fillcolor="#ADD8E6", color=black, fontcolor=black];
    msgDatos [label="Escribir\n\"Ingrese clave y\ncantidad\"", shape=parallelogram, fillcolor="#FFFACD", color=black, fontcolor=black];
    leerDatos [label="Leer PRO[I],\nCAN[I]", shape=parallelogram, fillcolor="#FFFACD", color=black, fontcolor=black];
    incremento [label="I ← I + 1", shape=box, fillcolor="#ADD8E6", color=black, fontcolor=black];
    msgError [label="Escribir\n\"Error en el dato\"", shape=egg, fillcolor="#FFFACD", color=black, fontcolor=black];
    conectorMayor [label="MAYOR", shape=pentagon, fillcolor="#ADD8E6", color=black, fontcolor=black];
    fin [label="FIN", shape=oval, fillcolor="#F08080", color=black, fontcolor=black];

    // Conexiones
    inicio -> msgN;
    msgN -> leerN;
    leerN -> validacion;
    validacion -> initI [label="Si"];
    validacion -> msgError [label="No"];
    initI -> converge1;
    converge1 -> ciclo;
    ciclo -> msgDatos [label="Si"];
    ciclo -> conectorMayor [label="No"];
    msgDatos -> leerDatos;
    leerDatos -> incremento;
    incremento -> converge1;
    msgError -> fin;
}
